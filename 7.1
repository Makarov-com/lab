#include <iostream>
using namespace std;
template<class T>
T ser(T arr[], int n){
    double suma=0;
    for(int i=0; i<n; i++){
       suma+=arr[i];
    }
    return suma/n;
};
int main()
{
    int n=3;
    int arr[3];
   cout << "Enter arr" << endl;
   for(int i=0; i<3; i++){
        cin>>arr[1];
   }
   cout << "Ser= " << ser(arr,n);
    return 0;
}
